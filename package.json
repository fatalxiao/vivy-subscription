{
    "name": "vivy-subscription",
    "author": "liangxiaojun",
    "version": "3.2.0",
    "description": "A Vivy Plugin to handle subscriptions in Vivy model to watch history or something else.",
    "main": "lib/index.js",
    "module": "es/index.js",
    "types": "es/index.d.ts",
    "files": [
        "lib",
        "es",
        "src",
        "LICENSE",
        "README.md"
    ],
    "scripts": {
        "clean": "rimraf lib es",
        "test": "cross-env NODE_ENV=test jest test/**.test.js",
        "build:commonjs": "tsc --project tsconfig.build.json --module commonjs --declaration false --outDir lib",
        "build:es": "tsc --project tsconfig.build.json --module esnext --declaration true --outDir es",
        "build": "npm run clean && npm run build:commonjs && npm run build:es",
        "do-publish": "npm run build && npm publish"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/fatalxiao/vivy-subscription"
    },
    "keywords": [
        "react",
        "redux",
        "subscription",
        "event",
        "vivy"
    ],
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/fatalxiao/vivy-subscription/issues"
    },
    "devDependencies": {
        "@types/jest": "^29.5.0",
        "@typescript-eslint/eslint-plugin": "^5.57.0",
        "@typescript-eslint/parser": "^5.57.0",
        "connected-react-router": "^6.9.3",
        "cross-env": "^7.0.3",
        "eslint": "^8.36.0",
        "eslint-plugin-jest": "^27.2.1",
        "history": "^4.10.1",
        "jest": "^29.5.0",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-router": "^5.2.1",
        "react-vivy": "^2.2.0",
        "rimraf": "^4.4.1",
        "ts-jest": "^29.0.5",
        "typescript": "^5.0.2",
        "vivy": "^3.3.2",
        "vivy-router": "^3.1.1"
    }
}
